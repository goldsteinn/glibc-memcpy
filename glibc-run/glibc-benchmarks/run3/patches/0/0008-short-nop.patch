From 92c5418d58751f246ec30fbe5ef88b98331058db Mon Sep 17 00:00:00 2001
From: Noah Goldstein <goldstein.w.n@gmail.com>
Date: Sun, 22 Aug 2021 08:09:43 -0400
Subject: [PATCH 08/11] short nop

---
 sysdeps/x86_64/multiarch/memmove-vec-unaligned-erms.S | 3 +--
 1 file changed, 1 insertion(+), 2 deletions(-)

diff --git a/sysdeps/x86_64/multiarch/memmove-vec-unaligned-erms.S b/sysdeps/x86_64/multiarch/memmove-vec-unaligned-erms.S
index 10ed0bfd6a..9d34091754 100644
--- a/sysdeps/x86_64/multiarch/memmove-vec-unaligned-erms.S
+++ b/sysdeps/x86_64/multiarch/memmove-vec-unaligned-erms.S
@@ -227,7 +227,6 @@ L(last_2x_vec):
 	VMOVU	%VEC(0), (%rdi)
 	VMOVU	%VEC(1), -VEC_SIZE(%rdi, %rdx)
 #if !defined USE_MULTIARCH || !IS_IN (libc)
-L(nop):
 	ret
 #else
 	VZEROUPPER_RETURN
@@ -423,7 +422,6 @@ L(skip_short_movsb_check):
 # endif
 	mov	%RDX_LP, %RCX_LP
 	rep	movsb
-L(nop):
 	ret
 	.p2align 4
 #endif
@@ -469,6 +467,7 @@ L(last_4x_vec):
 	VMOVU	%VEC(1), VEC_SIZE(%rdi)
 	VMOVU	%VEC(2), -VEC_SIZE(%rdi, %rdx)
 	VMOVU	%VEC(3), -(VEC_SIZE * 2)(%rdi, %rdx)
+L(nop):    
 	VZEROUPPER_RETURN
 	.p2align 4
 L(more_8x_vec):
-- 
