Subject: [PATCH 10/12] tmp

---
 sysdeps/x86_64/multiarch/bcmp-evex.S | 179 ++++++++++++++-------------
 1 file changed, 91 insertions(+), 88 deletions(-)

diff --git a/sysdeps/x86_64/multiarch/bcmp-evex.S b/sysdeps/x86_64/multiarch/bcmp-evex.S
index adf99bc44a..3e064361ed 100644
--- a/sysdeps/x86_64/multiarch/bcmp-evex.S
+++ b/sysdeps/x86_64/multiarch/bcmp-evex.S
@@ -71,9 +71,7 @@ ENTRY (BCMP)
 	kmovd	%k1, %eax
 	testl	%eax, %eax
 	jnz	L(return_neq0)
-
-    leaq    (%rdi, %rdx), %rcx
-	cmpq	$(VEC_SIZE * 2), %rdx
+	subq	$(VEC_SIZE * 2), %rdx
 	jbe	L(last_1x_vec)
 
 	/* Check second VEC no matter what.  */
@@ -84,7 +82,7 @@ ENTRY (BCMP)
 	jnz	L(return_neq0)
 
 	/* Less than 4 * VEC.  */
-	cmpq	$(VEC_SIZE * 4), %rdx
+	cmpq	$(VEC_SIZE * 2), %rdx
 	jbe	L(last_2x_vec)
 
 	/* Check third and fourth VEC no matter what.  */
@@ -101,28 +99,25 @@ ENTRY (BCMP)
 	jnz	L(return_neq0)
 
 	/* Go to 4x VEC loop.  */
-	cmpq	$(VEC_SIZE * 8), %rdx
+	cmpq	$(VEC_SIZE * 6), %rdx
 	ja	L(more_8x_vec)
 
 	/* Handle remainder of size = 4 * VEC + 1 to 8 * VEC without any
 	   branches.  */
+	addq	%rdx, %rsi
+	VMOVU	-(VEC_SIZE * 2)(%rsi), %YMM1
+	addq	%rdx, %rdi
+	vpxorq	-(VEC_SIZE * 2)(%rdi), %YMM1, %YMM1
+	VMOVU	-(VEC_SIZE * 1)(%rsi), %YMM2
 
-	VMOVU	-(VEC_SIZE * 4)(%rsi, %rdx), %YMM1
-	VMOVU	-(VEC_SIZE * 3)(%rsi, %rdx), %YMM2
 
-	/* Wait to load from s1 until addressed adjust due to
-	   unlamination.  */
+	vpternlogd $0xde, -(VEC_SIZE * 1)(%rdi), %YMM1, %YMM2
 
-	/* vpxor will be all 0s if s1 and s2 are equal. Otherwise it
-	   will have some 1s.  */
-	vpxorq	-(VEC_SIZE * 4)(%rcx), %YMM1, %YMM1
-	vpternlogd $0xde, -(VEC_SIZE * 3)(%rcx), %YMM1, %YMM2
-
-	VMOVU	-(VEC_SIZE * 2)(%rsi, %rdx), %YMM3
-	vpxorq	-(VEC_SIZE * 2)(%rcx), %YMM3, %YMM3
+	VMOVU	-(VEC_SIZE * 0)(%rsi), %YMM3
+	vpxorq	-(VEC_SIZE * 0)(%rdi), %YMM3, %YMM3
 	/* Or together YMM1, YMM2, and YMM3 into YMM3.  */
-	VMOVU	-(VEC_SIZE)(%rsi, %rdx), %YMM4
-	vpxorq	-(VEC_SIZE)(%rcx), %YMM4, %YMM4
+	VMOVU	-(VEC_SIZE * -1)(%rsi), %YMM4
+	vpxorq	-(VEC_SIZE * -1)(%rdi), %YMM4, %YMM4
 
 
 	vpternlogd $0xfe, %YMM2, %YMM3, %YMM4
@@ -133,71 +128,31 @@ ENTRY (BCMP)
 L(return_neq0):
 	ret
 
+	.p2align 4
 	/* Fits in padding needed to .p2align 5 L(less_vec).  */
 L(last_1x_vec):
-	VMOVU	-(VEC_SIZE * 1)(%rsi, %rdx), %YMM1
-	VPCMP	$4, -(VEC_SIZE * 1)(%rcx), %YMM1, %k1
+	VMOVU	-(VEC_SIZE * -1)(%rsi, %rdx), %YMM1
+	VPCMP	$4, -(VEC_SIZE * -1)(%rdi, %rdx), %YMM1, %k1
 	kmovd	%k1, %eax
 	ret
 
-	/* NB: p2align 5 here will ensure the L(loop_4x_vec) is also 32 byte
-	   aligned.  */
-	.p2align 5
-L(less_vec):
-	/* Check if one or less char. This is necessary for size = 0 but is
-	   also faster for size = 1.  */
-	cmpl	$1, %edx
-	jbe	L(one_or_less)
-
-	/* Check if loading one VEC from either s1 or s2 could cause a page
-	   cross. This can have false positives but is by far the fastest
-	   method.  */
-	movl	%edi, %eax
-	orl	%esi, %eax
-	andl	$(PAGE_SIZE - 1), %eax
-	cmpl	$(PAGE_SIZE - VEC_SIZE), %eax
-	jg	L(page_cross_less_vec)
-
-	/* No page cross possible.  */
-	VMOVU	(%rsi), %YMM2
-	VPCMP	$4, (%rdi), %YMM2, %k1
-	kmovd	%k1, %eax
-	/* Result will be zero if s1 and s2 match. Otherwise first set bit
-	   will be first mismatch.  */
-	bzhil	%edx, %eax, %eax
-	ret
-
-	/* Relatively cold but placing close to L(less_vec) for 2 byte jump
-	   encoding.  */
-	.p2align 4
-L(one_or_less):
-	jb	L(zero)
-	movzbl	(%rsi), %ecx
-	movzbl	(%rdi), %eax
-	subl	%ecx, %eax
-	/* No ymm register was touched.  */
-	ret
-	/* Within the same 16 byte block is L(one_or_less).  */
-L(zero):
-	xorl	%eax, %eax
-	ret
-
 	.p2align 4
 L(last_2x_vec):
-	VMOVU	-(VEC_SIZE * 2)(%rsi, %rdx), %YMM1
-	vpxorq	-(VEC_SIZE * 2)(%rcx), %YMM1, %YMM1
-	VMOVU	-(VEC_SIZE * 1)(%rsi, %rdx), %YMM2
-	vpternlogd $0xde, -(VEC_SIZE * 1)(%rcx), %YMM1, %YMM2
+	VMOVU	-(VEC_SIZE * 0)(%rsi, %rdx), %YMM1
+	vpxorq	-(VEC_SIZE * 0)(%rdi, %rdx), %YMM1, %YMM1
+	VMOVU	-(VEC_SIZE * -1)(%rsi, %rdx), %YMM2
+	vpternlogd $0xde, -(VEC_SIZE * -1)(%rdi, %rdx), %YMM1, %YMM2
 	VPTEST	%YMM2, %YMM2, %k1
 	kmovd	%k1, %eax
 	ret
 
+
 	.p2align 4
 L(more_8x_vec):
 	/* Set end of s1 in rdx.  */
-    addq    $-(VEC_SIZE * 4), %rcx
-	/* rsi stores s2 - s1. This allows loop to only update one pointer.
-	 */
+	leaq	-(VEC_SIZE * 2)(%rdi, %rdx), %rdx
+	/* rsi stores s2 - s1. This allows loop to only update one
+	   pointer.  */
 	subq	%rdi, %rsi
 	/* Align s1 pointer.  */
 	andq	$-VEC_SIZE, %rdi
@@ -220,34 +175,34 @@ L(loop_4x_vec):
 	vpxorq	(VEC_SIZE * 3)(%rdi), %YMM4, %YMM4
 
 	vpternlogd $0xfe, %YMM2, %YMM3, %YMM4
-    
+
 	VPTEST	%YMM4, %YMM4, %k1
 	kmovd	%k1, %eax
 	testl	%eax, %eax
 	jnz	L(return_neq2)
 	subq	$-(VEC_SIZE * 4), %rdi
-	cmpq	%rcx, %rdi
+	cmpq	%rdx, %rdi
 	jb	L(loop_4x_vec)
 
-	subq	%rcx, %rdi
-	VMOVU	(VEC_SIZE * 3)(%rsi, %rcx), %YMM4
-	vpxorq	(VEC_SIZE * 3)(%rcx), %YMM4, %YMM4
+	subq	%rdx, %rdi
+	VMOVU	(VEC_SIZE * 3)(%rsi, %rdx), %YMM4
+	vpxorq	(VEC_SIZE * 3)(%rdx), %YMM4, %YMM4
 	/* rdi has 4 * VEC_SIZE - remaining length.  */
 	cmpl	$(VEC_SIZE * 3), %edi
 	jae	L(8x_last_1x_vec)
 	/* Load regardless of branch.  */
-	VMOVU	(VEC_SIZE * 2)(%rsi, %rcx), %YMM3
-	/* Ternary logic to xor (VEC_SIZE * 2)(%rcx) with YMM3 while oring
-	   with YMM4. Result is stored in YMM4.  */
-	vpternlogd $0xf6, (VEC_SIZE * 2)(%rcx), %YMM3, %YMM4
+	VMOVU	(VEC_SIZE * 2)(%rsi, %rdx), %YMM3
+	/* Ternary logic to xor (VEC_SIZE * 2)(%rdx) with YMM3 while
+	   oring with YMM4. Result is stored in YMM4.  */
+	vpternlogd $0xf6, (VEC_SIZE * 2)(%rdx), %YMM3, %YMM4
 	cmpl	$(VEC_SIZE * 2), %edi
 	jae	L(8x_last_2x_vec)
 
-	VMOVU	VEC_SIZE(%rsi, %rcx), %YMM2
-	vpxorq	VEC_SIZE(%rcx), %YMM2, %YMM2
+	VMOVU	VEC_SIZE(%rsi, %rdx), %YMM2
+	vpxorq	VEC_SIZE(%rdx), %YMM2, %YMM2
 
-	VMOVU	(%rsi, %rcx), %YMM1
-	vpxorq	(%rcx), %YMM1, %YMM1
+	VMOVU	(%rsi, %rdx), %YMM1
+	vpxorq	(%rdx), %YMM1, %YMM1
 
 	vpternlogd $0xfe, %YMM1, %YMM2, %YMM4
 L(8x_last_1x_vec):
@@ -257,6 +212,53 @@ L(8x_last_2x_vec):
 L(return_neq2):
 	ret
 
+
+	/* NB: p2align 5 here will ensure the L(loop_4x_vec) is also 32
+	   byte aligned.  */
+	.p2align 5
+L(less_vec):
+	/* Check if one or less char. This is necessary for size = 0 but
+	   is also faster for size = 1.  */
+	cmpl	$1, %edx
+	jbe	L(one_or_less)
+
+	/* Check if loading one VEC from either s1 or s2 could cause a
+	   page cross. This can have false positives but is by far the
+	   fastest method.  */
+	movl	%edi, %eax
+	orl	%esi, %eax
+	andl	$(PAGE_SIZE - 1), %eax
+	cmpl	$(PAGE_SIZE - VEC_SIZE), %eax
+	jg	L(page_cross_less_vec)
+
+	/* No page cross possible.  */
+	VMOVU	(%rsi), %YMM2
+	VPCMP	$4, (%rdi), %YMM2, %k1
+	kmovd	%k1, %eax
+	/* Result will be zero if s1 and s2 match. Otherwise first set
+	   bit will be first mismatch.  */
+	bzhil	%edx, %eax, %eax
+	ret
+
+	/* Relatively cold but placing close to L(less_vec) for 2 byte
+	   jump encoding.  */
+	.p2align 4
+L(one_or_less):
+	jb	L(zero)
+	movzbl	(%rsi), %ecx
+	movzbl	(%rdi), %eax
+	subl	%ecx, %eax
+	/* No ymm register was touched.  */
+	ret
+	/* Within the same 16 byte block is L(one_or_less).  */
+L(zero):
+	xorl	%eax, %eax
+	ret
+
+
+
+
+
 	/* Relatively cold case as page cross are unexpected.  */
 	.p2align 4
 L(page_cross_less_vec):
@@ -278,8 +280,8 @@ L(page_cross_less_vec):
 L(between_16_31):
 	/* From 16 to 31 bytes.  No branch when size == 16.  */
 
-	/* Safe to use xmm[0, 15] as no vzeroupper is needed so RTM safe.
-	 */
+	/* Safe to use xmm[0, 15] as no vzeroupper is needed so RTM
+	   safe.  */
 	movups	(%rsi), %xmm1
 	vpcmpeqb (%rdi), %xmm1, %xmm1
 	movups	-16(%rsi, %rdx), %xmm2
@@ -290,10 +292,11 @@ L(between_16_31):
 	/* No ymm register was touched.  */
 	ret
 
+
 	.p2align 4,, 8
 L(between_9_15):
-	/* Safe to use xmm[0, 15] as no vzeroupper is needed so RTM safe.
-	 */
+	/* Safe to use xmm[0, 15] as no vzeroupper is needed so RTM
+	   safe.  */
 	vmovq	(%rdi), %xmm1
 	vmovq	(%rsi), %xmm2
 	vpcmpeqb %xmm1, %xmm2, %xmm3
@@ -305,7 +308,7 @@ L(between_9_15):
 	subl	$0xffff, %eax
 	/* No ymm register was touched.  */
 	ret
-    
+
 L(between_2_3):
 	/* From 2 to 3 bytes.  No branch when size == 2.  */
 	movzwl	(%rdi), %eax
@@ -316,7 +319,7 @@ L(between_2_3):
 	orl	%ecx, %eax
 	/* No ymm register was touched.  */
 	ret
-    
+
 
     .p2align 12
 END (BCMP)
