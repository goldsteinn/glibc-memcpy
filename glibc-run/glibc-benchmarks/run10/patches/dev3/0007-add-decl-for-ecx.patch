+++ b/sysdeps/x86_64/multiarch/memmove-vec-unaligned-erms.S
@@ -605,6 +605,7 @@ L(more_8x_vec_check):
 	leaq	(%rcx, %rdx), %r8
 	xorq	%rcx, %r8
 	shrq	$63, %r8
+    decl    %ecx
 	andl	$(PAGE_SIZE - 256), %ecx
 	addl	%r8d, %ecx
 #if !ALIGN_MOVSB && AVOID_SHORT_DISTANCE_REP_MOVSB
