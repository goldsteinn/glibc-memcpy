From 1da6fa91fc8fbdf8388c4d8be27cf37cc6b6ef91 Mon Sep 17 00:00:00 2001
From: Noah Goldstein <goldstein.w.n@gmail.com>
Date: Thu, 26 Aug 2021 17:17:18 -0400
Subject: [PATCH 8/8] tmp

---
 .../multiarch/memmove-vec-unaligned-erms.S    | 21 +++++++++++--------
 1 file changed, 12 insertions(+), 9 deletions(-)

diff --git a/sysdeps/x86_64/multiarch/memmove-vec-unaligned-erms.S b/sysdeps/x86_64/multiarch/memmove-vec-unaligned-erms.S
index 5d326a276c..0858ef4387 100644
--- a/sysdeps/x86_64/multiarch/memmove-vec-unaligned-erms.S
+++ b/sysdeps/x86_64/multiarch/memmove-vec-unaligned-erms.S
@@ -580,14 +580,6 @@ L(nop):
 	/* Align if doesn't cost too much code size.  */
 	.p2align 4,, 10
 L(more_8x_vec):
-	/* Check if non-temporal move candidate.  */
-#if (defined USE_MULTIARCH || VEC_SIZE == 16) && IS_IN (libc)
-	/* Check non-temporal store threshold.  */
-	cmp	__x86_shared_non_temporal_threshold(%rip), %RDX_LP
-	ja	L(large_memcpy_2x)
-#endif
-
-L(more_8x_vec_check):
 #if ALIGN_MOVSB || !AVOID_SHORT_DISTANCE_REP_MOVSB
 	VMOVU	(%rsi), %VEC(4)
 #endif
@@ -599,23 +591,34 @@ L(more_8x_vec_check):
 #else
 	jb	L(more_8x_vec_backward_test)
 #endif
+
+	/* Check if non-temporal move candidate.  */
+#if (defined USE_MULTIARCH || VEC_SIZE == 16) && IS_IN (libc)
+	/* Check non-temporal store threshold.  */
+	cmp	__x86_shared_non_temporal_threshold(%rip), %RDX_LP
+	ja	L(large_memcpy_2x)
+#endif
+
+
 	/* Entry if rdx is greater than movsb or stop movsb threshold but
 	   there is overlap with dst > src.  */
 
 	leaq	(%rcx, %rdx), %r8
 	xorq	%rcx, %r8
 	shrq	$63, %r8
-    decl    %ecx
+	decl	%ecx
 	andl	$(PAGE_SIZE - 256), %ecx
 	addl	%r8d, %ecx
 #if !ALIGN_MOVSB && AVOID_SHORT_DISTANCE_REP_MOVSB
 L(more_8x_vec_forward):
+L(more_8x_vec_check):
 	VMOVU	(%rsi), %VEC(4)
 	/* If jumped to here this will always be fall through.  */
 	jz	L(more_8x_vec_backward)
 #else
 	jz	L(more_8x_vec_backward)
 L(more_8x_vec_forward):
+L(more_8x_vec_check):
 #endif
 	/* Entry if rdx is greater than movsb or stop movsb threshold but
 	   there is overlap with dst > src.  */
-- 
